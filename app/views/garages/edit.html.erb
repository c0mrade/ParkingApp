<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Edit <%= @garage.name %> garage: </h1>
      <hr class='my-4'>
      <%= form_for @garage do |f| %>
        <% @garage.floors.each do |floor| %>
          <%= render 'fields', f: f %>
        <% end %>
        <div class="form-group">
          <%= f.submit "Update", class: 'btn btn-primary' %>
        </div>
      <% end %>
  </div>
</div>
